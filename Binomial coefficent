#include <stdio.h>

// Function to compute the binomial coefficient
int binomialCoefficient(int n, int k) {
    // Create a DP table to store results of subproblems
    int dp[n + 1][k + 1];

    // Fill the DP table
    for (int i = 0; i <= n; i++) {
        for (int j = 0; j <= k; j++) {
            // Condition I: C(n, 0) = 1 and C(n, n) = 1
            if (j == 0 || j == i) {
                dp[i][j] = 1;
            }
            // Condition II: C(n, k) = C(n-1, k-1) + C(n-1, k)
            else if (j > 0 && j < i) {
                dp[i][j] = dp[i - 1][j - 1] + dp[i - 1][j];
            } else {
                dp[i][j] = 0; // Default for invalid cases
            }
        }
    }

    // Return the binomial coefficient C(n, k)
    return dp[n][k];
}

int main() {
    int n = 8, k = 8;
    int result = binomialCoefficient(n, k);
    printf("The Binomial Coefficient C(%d, %d) is: %d\n", n, k, result);
    return 0;
}
